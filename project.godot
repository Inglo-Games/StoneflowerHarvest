[application]

config/name="Stoneflower Harvest"
run/main_scene="res://scenes/menu.tscn"
boot_splash/image="res://assets/images/gwj_splash.png"
config/icon="res://assets/icons/icon.png"

[debug]

shapes/collision/shape_color=Color( 0, 0.6, 0.701961, 0.501961 )

[display]

window/size/width=1920
window/size/height=1080
window/size/resizable=false
window/size/borderless=true
window/size/fullscreen=true
window/size/test_width=800
window/size/test_height=600
window/dpi/allow_hidpi=true
window/per_pixel_transparency/allowed=true
window/stretch/mode="viewport"
window/icon="res://icon.png"

[rendering]

quality/driver/driver_name="GLES2"
vram_compression/import_etc=true
vram_compression/import_etc2=false
environment/default_environment="res://default_env.tres"
