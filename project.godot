"class": "Connection",
"language": "GDScript",
"path": "res://scripts/connection.gd"
}, {
"base": "Control",
"class": "GameNode",
"language": "GDScript",
"path": "res://scripts/game_node.gd"
}, {
"base": "Node2D",
"class": "GameWorld",
"language": "GDScript",
"path": "res://scripts/game.gd"
}, {
"base": "GameNode",
"class": "Plunger",
